@using HackathonProje.Models.Entity
@model List<tblKullanici>
@{
    ViewBag.Title = "Login";
    Layout= "~/Views/Shared/MainLayout.cshtml";
}

@using (Html.BeginForm("Index","KullaniciLogin",FormMethod.Post)){
<div class="container mt-5 mb-5">
    
    <form>
        <div class="form-group">
            <label for="exampleInputEmail1">Email address</label>
            <input type="email" class="form-control" name="email" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
           
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Password</label>
            <input type="password" class="form-control" name="sifre" id="exampleInputPassword1" placeholder="Password">
        </div>

        <button type="submit" class="btn btn-primary btn-block">Giriş</button>
    </form>
  

</div>
}